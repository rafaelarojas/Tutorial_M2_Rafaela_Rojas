	FOREIGN KEY(&quot;cpf&quot;) REFERENCES &quot;pessoa&quot;(&quot;cpf&quot;)
);
CREATE TABLE IF NOT EXISTS &quot;dados&quot; (
	&quot;cpf&quot; INTEGER,
	&quot;endereço&quot;	TEXT NOT NULL,
	&quot;telefone&quot;	NUMERIC NOT NULL,
