	&quot;descrição&quot;	TEXT NOT NULL,
	&quot;id_cpf&quot;	NUMERIC NOT NULL,
	PRIMARY KEY(&quot;email&quot;),
	FOREIGN KEY(&quot;email&quot;) REFERENCES &quot;dados&quot;(&quot;email&quot;),
	FOREIGN KEY(&quot;id_cpf&quot;) REFERENCES &quot;pessoa&quot;(&quot;cpf&quot;),
	FOREIGN KEY(&quot;cpf&quot;) REFERENCES &quot;pessoa&quot; (&quot;cpf&quot;)
	
);
